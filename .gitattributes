# Auto detect text files and perform LF normalization
* text=auto
#include<stdio.h>
int main()
(
int a[5].b[5],c[5],i;
printf("enter the a values");
for(i=0;i<5;i++)
{
scanf("%d",&a[i]);
}
printf("enter the b values");
for(i=0;i<5;i++)
{
scanf("%d",&b[i]);
}
for(i=0;i<5;i++)
{
c[i]=a[i]+b[i];
}
for(i=0;i<5;i++)
{
printf("%d",c[i]);
}

